#ifndef UTILITIES_HPP
#define UTILITIES_HPP


#define POW0(x) (0)
#define POW1(x) (x)
#define POW2(x) (x*x)
#define POW3(x) (x*x*x)
#define POW4(x) (x*x*x*x)


constexpr inline void PRINTDBL(const char *s, const long double& d)
{
	std::cout.precision(std::numeric_limits<long double>::digits10+1);
	std::cout << s << ": " << d << '\n';
}






#endif // UTILITIES_HPP
